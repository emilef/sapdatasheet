;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.sapdatasheet.com. root.ns.sapdatasheet.com. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.
80	IN	PTR	ns.sapdatasheet.com.
80	IN	PTR	www.sapdatasheet.com.
